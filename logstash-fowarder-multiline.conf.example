{"files":[
	{"fields":
		{"cluster":"gus-qa","env":"qa","type":"hbase_regionserver"},
		"paths":["/var/log/java.log"],
		"multiline":
			{"enable":true,
			 "pattern":"(^\\s)",
         	 "negate":false,
	         "what":"previous"
			}},
	{"fields":
    	{"cluster":"gus-qa","env":"qa","type":"hbase_regionserver"},
         "paths":["/var/log/singleline.log"]
	}],
  "network":
	{"servers":["localhost:8514"],
     "ssl ca":"/ca.crt",
     "timeout":25}
}
